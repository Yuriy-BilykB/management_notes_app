{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "e2e:build": "detox build --configuration android.debug",
    "e2e:test": "detox test --configuration android.debug",
    "e2e:build:ios": "detox build --configuration ios.debug",
    "e2e:test:ios": "detox test --configuration ios.debug",
    "e2e:build:clean": "detox build --configuration android.debug --clean",
    "e2e:test:debug": "detox test --configuration android.debug --loglevel trace",
    "e2e:build:expo": "detox build --configuration android.debug --config-path detox.config.expo.js",
    "e2e:test:expo": "detox test --configuration android.debug --config-path detox.config.expo.js",
    "e2e:prebuild": "npx expo prebuild --clean",
    "e2e:mock": "jest e2e/mock.e2e.js --config e2e/jest.config.js",
    "e2e:simple": "jest e2e/simple.test.js --config e2e/jest.config.js",
    "e2e:working": "jest e2e/working.e2e.js --config e2e/jest.config.js",
    "e2e:basic": "jest e2e/basic.test.js --config e2e/jest.config.js",
    "e2e:detox-basic": "detox test --configuration android.debug --loglevel trace",
    "test:working": "npm run e2e:working",
    "test:simple": "npm run e2e:simple",
    "test:mock": "npm run e2e:mock",
    "test:basic": "npm run e2e:basic",
    "test:detox-basic": "npm run e2e:detox-basic"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.11.0",
    "class-validator": "^0.14.2",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.3.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "detox": "^20.40.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^30.0.5",
    "jest-circus": "^30.0.5",
    "typescript": "~5.8.3"
  },
  "private": true
}
